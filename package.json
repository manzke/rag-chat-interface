{
  "name": "rag-chat-interface",
  "version": "0.0.1",
  "description": "Modern chat interface for RAG systems",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint app/js/ server/",
    "format": "prettier --write 'app/**/*.{js,css}' 'server/**/*.js'",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "core-js": "^3.35.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.7",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manzke/rag-chat-interface.git"
  },
  "keywords": [
    "rag",
    "chat",
    "ai",
    "interface"
  ]
}